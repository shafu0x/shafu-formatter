pragma solidity ^0.8.26;

contract Escrow {
    function doMath(uint a, uint b) 
        external 
    {
        uint bb           = a + b;
        uint ccc          = a * b;
        uint d            = a - b;
        uint eeeeeeeee    = a / b;
        uint fff          = a + b * ccccc;
        uint gggggggggggg = a + b * ccccc;
    }
}